{
  "name": "cloud-explorer",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "6.2.x",
    "npm": "3.9.x"
  },
  "scripts": {
    "postinstall": "npm run build:fonts && npm run build",
    "start": "node .",
    "test": "jest",
    "test:watch": "watch 'npm test' src/ __tests__/",
    "build": "npm run build:js && npm run build:sass",
    "build:fonts": "mkdir -p dist/fonts && cp -R `node_modules`/font-awesome/fonts/* `node_modules`/npm-font-open-sans/fonts/* dist/fonts/",
    "build:js": "browserify src/js/App.js -d -o dist/cloud-explorer.js -t [ babelify ]",
    "build:sass": "node-sass --include-path src/sass --include-path node_modules/font-awesome/scss src/sass/style.scss dist/style.css",
    "watch": "live-reload dist/ --port 35729 & watch 'npm run build:js' src/js & watch 'npm run build:sass' src/sass"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.0.5",
    "body-parser": "^1.15.2",
    "browserify": "^13.0.1",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "dropbox": "^2.0.0",
    "express": "^4.0.0",
    "express-session": "^1.13.0",
    "font-awesome": "^4.6.3",
    "ftp": "^0.3.10",
    "node-sass": "^3.10.1",
    "npm-font-open-sans": "^1.0.3",
    "node_modules-path": "*",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "request": "^2.72.0",
    "serve-static": "^1.11.1",
    "unifile": "silexlabs/unifile#master"
  },
  "devDependencies": {
    "babel-jest": "^13.2.2",
    "jest-cli": "^13.2.3",
    "live-reload": "^1.1.0",
    "react-addons-test-utils": "^15.2.0",
    "watch": "^0.19.1"
  }
}
